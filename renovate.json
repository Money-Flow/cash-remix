{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": ["dev"],
  "packageRules": [
    {
      "matchDepTypes": ["dependencies", "devDependencies"],
      "automerge": true,
      "automergeType":  "pr",
      "automergeStrategy": "rebase"
    }
  ]
}
